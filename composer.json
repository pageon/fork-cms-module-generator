{
    "name": "pageon/fork-cms-module-generator",
    "description": "The generator for Fork modules",
    "type": "application",
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^3.2",
        "symfony/validator": "^3.2",
        "symfony/yaml": "^3.2",
        "symfony/security": "^3.2",
        "symfony/twig-bundle": "^3.2",
        "symfony/asset": "^3.2",
        "symfony/templating": "^3.2",
        "symfony/serializer": "^3.2",
        "vierbergenlars/php-semver": "^3.0",
        "matthiasnoback/symfony-console-form": "^2.2",
        "symfony/finder": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "ModuleGenerator\\": [
                "src/"
            ],
            "App\\": [
                "app/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModuleGenerator\\Tests\\": "test/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jelmer Prins",
            "email": "jelmer@pageon.be"
        },
        {
            "name": "Bruno Vitorino",
            "email": "bruno@vitorino.me"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^3.2",
        "behat/behat": "^3.3",
        "beberlei/assert": "^2.7",
        "squizlabs/php_codesniffer": "^2.8",
        "sebastian/diff": "^1.4"
    },
    "scripts": {
        "test": "vendor/bin/behat"
    }
}
